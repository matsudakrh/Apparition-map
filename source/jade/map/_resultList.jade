section#resultList.page
	.page-inner
		form.header-form(method="post", name="geoTest", action="/map/searchList.html", ng-submit="MainC.searchKeyword(MainC.searchPlaceText)")
			a.back-btn(href="#top")
				i.material-icons arrow_back
			input#placeName(type="text", name="geoName", ng-model="MainC.searchPlaceText")
			input(type="submit", value="検索")
		ul.result-list
			li.result-item(ng-repeat="item in MainC.searchList track by item.place_id",
					ng-click="MainC.viewGeoMap(item.geometry.location.lat(), item.geometry.location.lng(), item.place_id, $index )")
				p.address {{item.formatted_address}}
				.latlng
					p.lat {{item.geometry.location.lat()}}
					p.lng {{item.geometry.location.lng()}}
				hr